{
  "name": "vue-sfc-analyzer-webpack-plugin",
  "version": "1.0.0",
  "description": "Webpack plugin for analyzing Vue SFC",
  "main": "dist/VueSFCAnalyzerWebpackPlugin.js",
  "typings": "dist/types/VueSFCAnalyzerWebpackPlugin.d.ts",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
