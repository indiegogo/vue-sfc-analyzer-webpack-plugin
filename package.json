    "test:watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/indiegogo/webpack-vue-sfc-analyzer-plugin.git"
  },
  "keywords": [
    "webpack",
    "plugin",
    "vue"
  ],
  "author": "Indiegogo, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/indiegogo/webpack-vue-sfc-analyzer-plugin/issues"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "mkdirp": "^0.5.1",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "12.6.1",
    "@types/webpack": "4.4.34",
    "@types/winston": "^2.3.8",
    "css-loader": "3.0.0",
    "jest": "24.8.0",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "typescript": "3.5.3",
    "vue": "^2.5.16",
    "vue-loader": "15.7.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "4.35.3"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "modulePathIgnorePatterns": [
      "/test/fixtures/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
