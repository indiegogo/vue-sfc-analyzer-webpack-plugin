{
  "name": "vue-sfc-analyzer-webpack-plugin",
  "version": "1.0.0-beta.2",
  "description": "Webpack plugin for analyzing Vue SFC",
  "main": "dist/VueSFCAnalyzerWebpackPlugin.js",
  "typings": "dist/types/VueSFCAnalyzerWebpackPlugin.d.ts",
  "engines": {
    "node": ">=6.11.5"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/indiegogo/webpack-vue-sfc-analyzer-plugin.git"
  },
  "keywords": [
    "webpack",
    "plugin",
    "vue"
  ],
  "author": "Indiegogo, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/indiegogo/webpack-vue-sfc-analyzer-plugin/issues"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "mkdirp": "^0.5.1",
    "webpack": "^4.11.0",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^9.4.7",
    "@types/webpack": "^4.4.2",
    "@types/winston": "^2.3.8",
    "css-loader": "^0.28.10",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.1",
    "ts-loader": "^4.4.1",
    "typescript": "^2.8.3",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "modulePathIgnorePatterns": [
      "/test/fixtures/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ]
  }
}
